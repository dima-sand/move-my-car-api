{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "target": "ES2022",
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "lib": ["ES2017"]
  },
  "include": ["src/**/*.ts",]
}
